{
  "mcpServers": {
    "linear-server": {
      "type": "sse",
      "url": "https://mcp.linear.app/sse"
    },
    "prometheus": {
      "command": "/opt/homebrew/bin/docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "-e",
        "PROMETHEUS_URL=https://prometheus.private.prefect.cloud/",
        "ghcr.io/pab1it0/prometheus-mcp-server:latest"
      ]
    },
    "prefect-docs": {
      "type": "stdio",
      "command": "https://docs.prefect.io/mcp",
      "args": [],
      "env": {}
    }
  }
}
